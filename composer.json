{
  "name": "alex-patterson-webdev/laminas-entity",
  "version": "1.0.0",
  "minimum-stability": "dev",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Alex Patterson",
      "email": "alex.patterson.webdev@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.4",
    "laminas/laminas-component-installer" : "^2",
    "laminas/laminas-servicemanager" : "^3.4.",
    "laminas/laminas-modulemanager" : "^2.8.",
    "doctrine/doctrine-laminas-hydrator" : "^2",
    "alex-patterson-webdev/doctrine-entity-repository": "dev-feature/0.0.3",
    "alex-patterson-webdev/laminas-factory": "dev-feature/0.2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.1",
    "squizlabs/php_codesniffer": "3.*",
    "phpmd/phpmd" : "@stable",
    "friendsofphp/php-cs-fixer": "^2.16"
  },
  "autoload": {
    "psr-4": {
      "Arp\\LaminasEntity\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "ArpTest\\LaminasEntity\\": "test/phpunit/"
    }
  },
  "repositories": [
    {
      "type": "composer",
      "url": "http://dev.satis.local"
    }
  ],
  "scripts": {
    "test": "php vendor/bin/phpunit --coverage-clover=coverage.xml"
  },
  "config": {
    "secure-http": false,
    "preferred-install": "dist",
    "optimize-autoloader": true,
    "sort-packages": true
  },
  "extra": {
    "laminas": {
      "component ": "Arp\\LaminasEntity"
    }
  }
}
